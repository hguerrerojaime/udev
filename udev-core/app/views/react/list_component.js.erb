import React from 'react';

import Panel from 'react-jspm/commons/Panel';
import Well from 'react-jspm/commons/Well';
import LinkButton from 'react-jspm/commons/LinkButton';

export default class <%=@domain_object.class_name%>_List extends React.Component {

  render() {
    return (
      <Panel brand="info" title="<%=@domain_object.plural_label%>">
         <Well size="sm">
            <LinkButton
                  href="/provider/new"
                  icon="fa fa-plus"
                  brand="success" label="New <%=@domain_object.label%>"
            />
         </Well>
      </Panel>
    );
  }

}
